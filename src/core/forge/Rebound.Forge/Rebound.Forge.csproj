<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0-windows10.0.26100.0</TargetFramework>
		<TargetPlatformMinVersion>10.0.22000.0</TargetPlatformMinVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<UseUwp>true</UseUwp>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <AdditionalFiles Include="NativeMethods.json" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="CommunityToolkit.Mvvm" Version="8.4.0" />
		<PackageReference Include="TerraFX.Interop.Windows" Version="10.0.26100.4" />
		<PackageReference Include="Microsoft.Windows.CsWin32" Version="0.3.238">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\Rebound.Core.Native\Rebound.Core.Native.csproj" />
	  <ProjectReference Include="..\..\Rebound.Core.Storage\Rebound.Core.Storage.csproj" />
	  <ProjectReference Include="..\..\Rebound.Core.UI\Rebound.Core.UI.csproj" />
	  <ProjectReference Include="..\..\Rebound.Core\Rebound.Core.csproj" />
	</ItemGroup>

</Project>
